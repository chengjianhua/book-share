{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "src/**/*.js"
  ],
  "dontLoad": [
    "node_modules"
  ],
  "plugins": {
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {},
    "es_modules": {}
  }
}